{
    "name": "Student Count with YOLOv10 DeepStream Optimized",
    "image": "nvcr.io/nvidia/deepstream:7.0-triton-multiarch",
    "runArgs": [
        "--net=host",
        "--runtime=nvidia",
        "--gpus=all",
        "--privileged",
        "-e", "DISPLAY=${localEnv:DISPLAY}",
        "--device", "/dev/snd",
        "-v", "/tmp/.X11-unix:/tmp/.X11-unix"
    ],
    "workspaceFolder": "/workspace",
    "workspaceMount": "source=/home/hamza/PycharmProjects/Student-Count-YOLOv10,target=/workspace,type=bind,consistency=cached",
    "remoteUser": "root",
    "mounts": [
        "source=/var/run/docker.sock,target=/var/run/docker.sock,type=bind"
    ],
    "postCreateCommand": "apt-get update && apt-get install -y x11-apps mesa-utils"
}
